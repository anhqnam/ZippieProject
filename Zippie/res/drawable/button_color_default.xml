<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
       <shape>
         <corners android:radius="3dip" />
         <stroke android:width="1dip" android:color="@color/button_default_notpress" />
         <gradient android:angle="-90" android:startColor="@color/button_default_notpress" android:endColor="@color/button_default_notpress"  />         
      </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
         <corners android:radius="3dip" />
         <stroke android:width="1dip" android:color="@color/button_default_press" />
         <gradient android:angle="-90" android:startColor="@color/button_default_press" android:endColor="@color/button_default_press"  /> 
      </shape>
    </item>
</selector>
