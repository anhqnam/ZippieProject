<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
       <shape>
         <corners android:radius="3dip" />
         <stroke android:width="1dip" android:color="@color/createprofile_background_logo_facebook_notpress_col" />
         <solid android:color="@color/createprofile_background_logo_facebook_notpress_col"/>
      </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
         <corners android:radius="3dip" />
         <stroke android:width="1dip" android:color="@color/createprofile_background_logo_facebook_press_col" />
         <solid android:color="@color/createprofile_background_logo_facebook_press_col"/>
      </shape>
    </item>
</selector>
