<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="false">
        <shape android:shape="oval">
            <solid android:color="@color/topmenu_background_menu_item_notpress_col"/>
            <stroke android:width="2dp"
                android:color="@color/topmenu_background_menu_item_stroke_notpress_col"/>
        </shape>
    </item>
    <item android:state_pressed="true"> 
        <shape android:shape="oval">
            <solid android:color="@color/topmenu_background_menu_item_stroke_press_col"/>
            <stroke android:width="2dp"
                android:color="@color/topmenu_background_menu_item_stroke_press_col"/>
        </shape>
    </item>
</selector>
