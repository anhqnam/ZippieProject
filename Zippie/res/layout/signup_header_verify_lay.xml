<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/common_button_default_notpress_col"
    android:gravity="center_vertical"
    android:orientation="horizontal" >
    <ImageView android:id="@+id/signup_header_verify_back_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingLeft="10dp"
        android:src="@drawable/common_back_icon"/>
    <TextView 
        android:id="@+id/signup_header_verify_title_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textStyle="bold"
        android:textColor="#fff"
        android:textSize="@dimen/text_size_48_point"
        android:paddingLeft="10dp"/>

</LinearLayout>
