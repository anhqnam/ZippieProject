<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="contacts_info_number_row_text">
        <item name="android:textColor">@color/text_noneditable</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:textAppearance">?android:attr/textAppearanceSmall</item>
    </style>

    <style name="contacts_info_number_row_text_native">
        <item name="android:textColor">@color/text_editable</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:textAppearance">?android:attr/textAppearanceSmall</item>
    </style>

    <style name="contacts_info_number_row_text_vippie">
        <item name="android:textColor">@color/text_row_primary</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:textAppearance">?android:attr/textAppearanceSmall</item>
    </style>

    <style name="dialpad_button" parent="button">
        <item name="android:background">@drawable/button_dialpad</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:padding">5dp</item>
        <item name="android:textSize">15sp</item>
        <item name="android:layout_margin">1dp</item>
    </style>

    <style name="registration_title" parent="android:Widget.TextView">
        <item name="android:textAppearance">@android:style/TextAppearance.Medium</item>
    </style>
		
    <style name="intro_text" parent="android:Widget.TextView">
        <item name="android:textAppearance">@android:style/TextAppearance.Small</item>
    </style>
    
    <style name="registration_number_navigation" parent="android:Widget.TextView">
        <item name="android:textAppearance">@android:style/TextAppearance.Small</item>
    </style>
    
    <dimen name="registration_btn_size">34dp</dimen>
</resources>