<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="dialpad_button" parent="button">
        <item name="android:background">@drawable/button_dialpad</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:padding">5dp</item>
        <item name="android:textSize">20sp</item>
        <item name="android:layout_margin">1dp</item>
    </style>
	
    <style name="registration_title" parent="android:Widget.TextView">
        <item name="android:textAppearance">@android:style/TextAppearance.Large</item>
    </style>
    	
    <style name="intro_text" parent="android:Widget.TextView">
        <item name="android:textAppearance">@android:style/TextAppearance.Small</item>
    </style>
    
</resources>